    <toolbar class="center">
        <item type="button" icon="hangup" action="hangup" params="confirm=1" keep-uri keep-referrer >Hangup</item>
        <item type="button" icon="cancel" action="cancel">Cancel</item>
    </toolbar>
</dialog>
